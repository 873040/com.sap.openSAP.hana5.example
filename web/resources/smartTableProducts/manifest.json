{
  "_version": "1.1.0",
  "start_url": "index.html",
  "sap.app": {
    "dataSources": {
      "mainServiceAnnotation": {
        "uri": "/node/annotations/com.sap.openSAP.hana5.example.services.ProductsType/MDViews.ProductsConsumption/",
        "type": "ODataAnnotation"
      },
      "mainService": {
        "uri": "/xsodata/products.xsodata/",
        "type": "OData",
        "settings": {
          "annotations": [
            "mainServiceAnnotation"
          ],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui5": {
    "models": {
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v2.ODataModel"
      }
    },
    "config": {
      "sapFiori2Adaptation": true
    },
    "rootView": "sap.openSAP.smarttableProducts.view.SmartTable",
    "handleValidation": true
  }
}